      <div style="text-align: right">
	Logout
      </div>
      <hr>
        {% block body %}{% endblock %}
        {% block javascripts %}{% endblock %}
      <hr>
      <center>
        IRMA - Indice Restituzioni Materiale A
	</center>
    </body>
</html>
